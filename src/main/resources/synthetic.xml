<synthetic xsi:schemaLocation="http://www.xebialabs.com/deployit/synthetic synthetic.xsd" xmlns="http://www.xebialabs.com/deployit/synthetic" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<type-modification type="jbossdm.CliBasedContainer">
		<property name="startScript" required="true" description="Start script to start the container (must be located on the server)"/>
		<property name="stopScript" required="true" description="Stop script to start the container (must be located on the server)"/>
	</type-modification>

	<type-modification type="jbossdm.CliManagedDeployed">
		<property name="restartContainer" required="true" default="false" kind="boolean" />
	</type-modification>
</synthetic>
